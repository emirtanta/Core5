@model IEnumerable<XCafeProject.Models.Galeri>
@{
    ViewData["Title"] = "Galeri";
    Layout = "~/Views/Shared/_SiteLayout.cshtml";
}

<!-- Title Page -->
	<section class="bg-title-page flex-c-m p-t-160 p-b-80 p-l-15 p-r-15" style="background-image: url(/pato-master/images/bg-title-page-02.jpg);">
		<h2 class="tit6 t-center">
			Galeri
		</h2>
	</section>



	<!-- Gallery -->
	<div class="section-gallery p-t-118 p-b-100">
		

		<div class="wrap-gallery isotope-grid flex-w p-l-25 p-r-25">
			<!-- - -->
			@foreach (var item in Model)
		   {
			   <div class="item-gallery isotope-item bo-rad-10 hov-img-zoom events guests">
				<img src="@item.Image" alt="IMG-GALLERY">

				<div class="overlay-item-gallery trans-0-4 flex-c-m">
					<a class="btn-show-gallery flex-c-m fa fa-search" href="~/pato-master/images/photo-gallery-13.jpg" data-lightbox="gallery"></a>
				</div>
			</div>
		   }
			

			
		</div>

		
	</div>

